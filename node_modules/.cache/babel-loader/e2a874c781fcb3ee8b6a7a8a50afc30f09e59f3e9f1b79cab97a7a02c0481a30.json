{"ast":null,"code":"import TheCarousel from '@/components/TheCarousel.vue';\nimport TheSlider from '@/components/TheSlider.vue';\nimport featureContentBox from '@/featureContentBox'; //import the feature contents\nexport default {\n  components: {\n    TheCarousel,\n    TheSlider\n  },\n  data() {\n    return {\n      featureContents: featureContentBox.featureContents\n    };\n  },\n  // mounted() {\n  //        this.links = links\n  //         }\n  setup() {\n    const carouselSlides = ['lady.jpeg', 'lady-2.jpeg', 'lady-3.jpeg'];\n    return {\n      carouselSlides\n    };\n  }\n};","map":{"version":3,"names":["TheCarousel","TheSlider","featureContentBox","components","data","featureContents","setup","carouselSlides"],"sources":["/Users/mac/Desktop/50-Vue-Projects/worku/src/Views/FeaturesPage.vue"],"sourcesContent":["<template>\n  <section>\n       <!-- add a slider here -->\n       <div>\n              <TheCarousel class=\"carousel\"  v-slot=\"{currentSlide}\">\n                     <TheSlider v-for=\"(slide, index) in carouselSlides\" :key=\"index\">\n                            <div v-show=\"currentSlide === index + 1\" class=\"slide-info\">\n                                  \n                                   <img :src=\"require(`../assets/${slide}`)\"/>\n                            </div>\n                     </TheSlider>\n              </TheCarousel>\n       </div>\n\n       <!-- starts of feature content box -->\n       <v-row>\n       <v-col class=\"contentbox\" v-for=\"(contentbox, id) in featureContents\" :key=\"id\" col=\"2\">\n         <div>\n          <img :src=\"require(`@/assets/featuresImages/${contentbox.image}`)\"/>\n        <h2>{{ contentbox.title }}</h2>\n        <p> {{contentbox.description}}</p>\n        <a :href=\"contentbox.learnMore\">Learn More</a>\n       </div>\n       </v-col>\n    </v-row>\n </section>\n</template>\n\n<script>\nimport TheCarousel from '@/components/TheCarousel.vue';\nimport TheSlider from '@/components/TheSlider.vue';\nimport featureContentBox from '@/featureContentBox';  //import the feature contents\nexport default {\n       components:{\n              TheCarousel,TheSlider\n       },\n\n       data(){\n           return{\n              featureContents: featureContentBox.featureContents\n           }   \n           \n       },\n       // mounted() {\n       //        this.links = links\n       //         }\n         setup(){\n              const carouselSlides = ['lady.jpeg', 'lady-2.jpeg','lady-3.jpeg']\n\n              return{carouselSlides}\n       }\n}\n</script>\n\n<style scoped>\n.carousel{\n       position: relative;\n       overflow: hidden;\n       max-height: 100vh;\n       height:100vh;\n}\n.slide-info{\n       position: absolute;\n       width:100%;\n       height:90%;\n       top:0;\n       left:0;\n}\nimg{\n       min-width: 100%;\n       height:90vh;\n       background-position: center center;\n       object-fit: cover;\n}\n\n.contentbox{\n  display: flex;\n  flex-direction: column;\n}\n</style>"],"mappings":"AA6BA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,iBAAgB,MAAO,qBAAqB,EAAG;AACtD,eAAe;EACRC,UAAU,EAAC;IACJH,WAAW;IAACC;EACnB,CAAC;EAEDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACHC,eAAe,EAAEH,iBAAiB,CAACG;IACtC;EAEJ,CAAC;EACD;EACA;EACA;EACEC,KAAKA,CAAA,EAAE;IACF,MAAMC,cAAa,GAAI,CAAC,WAAW,EAAE,aAAa,EAAC,aAAa;IAEhE,OAAM;MAACA;IAAc;EAC5B;AACP"},"metadata":{},"sourceType":"module","externalDependencies":[]}